{
  "homepage": "https://nyambura00.github.io/NEAR-meetups",
  "name": "near-meetups",
  "version": "1.0.0",
  "description": "Creating a meetups Dapp using NEAR protocol",
  "author": "nyambura00 <winnienyambura14@gmail.com>",
  "license": "MIT",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "build:contract": "asb",
    "build:contract:debug": "asb --target debug",
    "dev:deploy:contract": "near dev-deploy --wasmFile=\"./out/main.wasm\" && echo \"# This file used by webpack\nCONTRACT_NAME=$(cat neardev/dev-account)\" > .env",
    "test:contract": "yarn build:contract:debug && asp",
    "test": "yarn build:contract:debug && asp",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build"
  },
  "devDependencies": {
    "@commitlint/cli": "11.0.0",
    "@commitlint/config-conventional": "11.0.0",
    "@types/node": "14.14.28",
    "@types/react": "17.0.2",
    "@types/react-dom": "17.0.1",
    "@typescript-eslint/eslint-plugin": "4.15.1",
    "@typescript-eslint/parser": "4.15.1",
    "commitizen": "4.2.3",
    "cz-conventional-changelog": "3.3.0",
    "eslint": "7.32.0",
    "eslint-config-airbnb": "18.2.1",
    "eslint-config-next": "12.0.3",
    "eslint-config-prettier": "7.2.0",
    "eslint-import-resolver-typescript": "2.4.0",
    "eslint-plugin-import": "2.22.1",
    "eslint-plugin-jsx-a11y": "6.4.1",
    "eslint-plugin-prettier": "3.3.1",
    "eslint-plugin-react": "7.22.0",
    "eslint-plugin-react-hooks": "4.2.0",
    "husky": "5.0.9",
    "lint-staged": "10.5.4",
    "near-sdk-as": "^3.2.3",
    "prettier": "2.2.1"
  },
  "dependencies": {
    "@fontsource/inter": "4.2.1",
    "@svgr/webpack": "5.5.0",
    "asbuild": "^0.2.0",
    "asp": "^0.0.2",
    "autoprefixer": "^10.4.5",
    "gh-pages": "^3.2.3",
    "lint": "0.7.0",
    "near-api-js": "^0.44.2",
    "near-cli": "3.2.0",
    "next": "10.0.7",
    "next-seo": "4.20.0",
    "postcss": "^8.4.12",
    "react-bootstrap": "^2.3.0",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-router-dom": "^6.3.0",
    "react-tsparticles": "^2.0.6",
    "tailwindcss": "^3.0.24",
    "twind": "0.15.4",
    "typescript": "^4.6.3",
    "uuid": "8.3.2",
    "react-spinners": "^0.11.0"
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "lint-staged": {
    "./src/**/*.{ts,js,jsx,tsx}": [
      "yarn lint --fix",
      "yarn format"
    ]
  }
}
