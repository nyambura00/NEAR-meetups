{
  "name": "near-meetups",
  "version": "1.0.0",
  "description": "Creating a meetups Dapp using NEAR protocol",
  "main": "index.js",
  "author": "nyambura00 <winnienyambura14@gmail.com>",
  "license": "MIT",
  "scripts": {
    "build": "yarn build:contract",
    "build:contract": "asb",
    "build:contract:debug": "asb --target debug",
    "deploy": "yarn build && near deploy",
    "dev": "yarn build:contract:debug && near dev-deploy ./build/debug/near-meetups.wasm",
    "test": "yarn build:contract:debug && asp"
  },
  "devDependencies": {
    "near-sdk-as": "^3.2.3"
  },
  "dependencies": {
    "asbuild": "^0.2.0",
    "asp": "^0.0.2",
    "typescript": "^4.6.3"
  }
}
